
config smartdns
	option server_name 'smartdns'
	option dualstack_ip_selection '1'
	option tcp_server '0'
	option seconddns_tcp_server '0'
	option coredump '0'
	option port '6053'
	option rr_ttl '3600'
	option rr_ttl_min '5'
	option resolve_local_hostnames '1'
	option ipv6_server '1'
	option force_aaaa_soa '0'
	option force_https_soa '0'
	option auto_set_dnsmasq '0'
	option speed_check_mode 'tcp:443,tcp:80,ping'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_rule_soa '0'
	option seconddns_server_group 'outdns'
	option enable_auto_update '0'
	option prefetch_domain '1'
	option cache_size '51200'
	option seconddns_no_dualstack_selection '0'
	option seconddns_force_aaaa_soa '0'
	option serve_expired '1'
	option seconddns_no_cache '0'
	option seconddns_port '6054'
	option seconddns_no_speed_check '0'
	option old_port '6053'
	option old_enabled '0'
	option old_auto_set_dnsmasq '0'

config server
	option enabled '1'
	option name '腾讯'
	option ip 'dot.pub'
	option type 'tls'

config server
	option enabled '1'
	option name '阿里'
	option ip 'dns.alidns.com'
	option type 'tls'

config server
	option enabled '1'
	option name 'onedns'
	option ip 'doh.onedns.net/dns-query'
	option type 'https'

